generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Usuario {
    id Int @id @default(autoincrement())
    nome String @db.VarChar(255)
    email String @unique @db.VarChar(160)
    telefone String @unique @db.Char(11)
    senha String @db.Char(60)
}